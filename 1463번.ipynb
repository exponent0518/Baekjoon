   "source": [
    "# 1463ë²ˆ\n",
    "n=int(input())\n",
    "lst = [0] * (n+1)\n",
    "\n",
    "for i in range(2,n+1):\n",
    "    lst[i] = lst[i-1] + 1\n",
    "    if i % 3 == 0:\n",
    "        lst[i] = min(lst[i], lst[i//3] + 1)\n",
    "    if i % 2 == 0:\n",
    "        lst[i] = min(lst[i], lst[i//2] + 1)\n",
    "print(lst[n])"
   ]
  }
